<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootPane" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.jscancella.RootController">
  <children>
    <MenuBar maxWidth="2.147483647E9" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#handleAddFolderButtonAction" text="Add Folder to Scan" />
            <MenuItem mnemonicParsing="false" onAction="#handleQuitAction" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#handleAboutAction" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <ProgressBar fx:id="progressBar" prefHeight="20.0" prefWidth="2.147483647E9" progress="0.0" />
      <SplitPane dividerPositions="0.29797979797979796" prefHeight="2.147483647E9" prefWidth="2.147483647E9">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <ScrollPane fx:id="foldersToScanScrollPane" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" text="Duplicate Files:" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <ScrollPane fx:id="duplicateFilesFoundScrollPane" layoutY="43.0" prefHeight="230.0" prefWidth="446.0" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
      <ButtonBar buttonMinWidth="50.0" prefHeight="40.0" prefWidth="2.147483647E9">
        <buttons>
          <Button fx:id="deleteButton" disable="true" mnemonicParsing="false" onAction="#handleDeleteButtonAction" text="Delete Files" />
            <Button fx:id="scanFoldersButton" disable="true" mnemonicParsing="false" onAction="#handleScanFoldersButtonAction" text="Scan Folders" />
            <Button fx:id="addFolderButton" mnemonicParsing="false" onAction="#handleAddFolderButtonAction" text="Add Folder to Scan" />
            <Button disable="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="256.0" text="Remove Folder" />
        </buttons>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ButtonBar>
  </children>
</VBox>
